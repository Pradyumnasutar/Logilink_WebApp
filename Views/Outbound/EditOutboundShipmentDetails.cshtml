@model LeS_LogiLink_WebApp.Controllers.OutboundShipmentUpdateModal
@{
    ViewData["Title"] = "Update Outbound Shipment";
    Layout = "~/Views/Shared/_BaseMasterDetail.cshtml";
}
@section Scripts {
    <style>

        .detail {
            position: absolute;
            right: 32px;
        }

        .disableClick {
            pointer-events: none;
        }

        .datatable-header-cell {
            border-radius: 10px; /* Adjust the value as per your preference */
            /* Other styling properties */
        }

        .button-column {
            display: flex;
            justify-content: center;
            margin: 5px;
            padding: 8px;
            align-items: center;
            background-color: #787373; /* Primary button color */
            color: white;
            border-radius: 20px;
            text-align: center;
            cursor: pointer;
            transition: background-color 0.3s;
        }

        .select2-container .select2-selection--multiple .select2-selection__rendered {
            display: contents;
            flex-wrap: wrap;
            max-width: 100%;
            box-sizing: border-box;
        }

        .my-custom-dialog {
            width: 40%; /* Adjust width as needed */
        }

    </style>

    <script type="module" src="~/assets/js/custom/Outbound/EditOutboundShipmentDetails.js?ver=12345"></script>

    <script>
        var ShipmentId = @ViewBag.Key;
        var Model = @Html.Raw(Json.Serialize(@Model.UpdateShipmentOrderModal));
        var isbuyer = @Html.Raw(Json.Serialize(@Model.IsBuyer));
        var shipdeliveryorders = @Html.Raw(Json.Serialize(@Model.ShipmentDeliveryorders));
    </script>


}
@section ActionList
    {

    @{
        if (Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid == 2||Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid == 11)//draft
        {
            if (Model.ShipmentDeliveryorders.Count > 0)
            {
                <button class="btn btn-sm btn-success ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnReleaseShipment" style="display:none;">
                    <i class="fas fa-paper-plane fs-3 me-2"></i>Release
                </button>

            }
            <button class="btn btn-sm btn-success ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnassigndo" style="display:none;">
                <i class="fa fa-lock fs-3 me-2"></i>Assign DO
            </button>

            <button class="btn btn-sm btn-primary ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnCancelShipment" style="display:none;">
                <i class="fa fa-ban fs-3 me-2" aria-hidden="true"></i>Cancel
            </button>
            <div class="position-relative p-3" data-bs-placement="bottom" data-bs-toggle="tooltip" title="Save">
                <button class="btn btn-sm btn-success ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnSaveoutboundshipment" style="display:none;">
                    <i class="fas fa-pen fs-3 me-2"></i> Save
                </button>
            </div>
        }

        if (Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid == 5)//ready to ship
        {

            <button class="btn btn-sm btn-success ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnUnReleaseShipment" style="display:none;">
                <i class="fa fa-unlock fs-3 me-2"></i>Unrelease
            </button>

        }

        if (Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid != 4 && Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid != 2 && Model.UpdateShipmentOrderModal.shipment_Info.shipment_statusid != 11)
        {
            <button class="btn btn-sm btn-primary ModuleAccess" data-module="1" data-access_level="2,3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" id="btnPrintShipmentOrder">
                <i class="bi bi-printer fs-3 me-2"></i> Print
            </button>
        }


    }

    <button class="btn btn-sm btn-danger" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" onclick="NavigatePage('/Outbound/OutboundShipmentsList')">
        <i class="fas fa-arrow-circle-left fs-3 me-2"></i> Back
    </button>
}



