@using LeS_LogiLink_WebApp.Models
@using Microsoft.AspNetCore.Http;
@inject IEpodUserDefaultData UserData
<!DOCTYPE html>
<html>
<head>
    <title>LogiLink</title>
    <meta charset="utf-8" />
    <meta name="description" content="Lighthouse eSolutions" />
    <meta name="keywords" content="LeS, Logistics Management,Logistics,Management" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="article" />
    <meta property="og:type" content="article" />
    <meta property="og:title" content="Logistics Management" />
    <meta property="og:site_name" content="Logistics Management" />

    <link rel="shortcut icon" id="shorticon" href="#" />
    <!--begin::Fonts-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />
    <!--end::Fonts-->

    <link href="~/assets/media/slick/slick.css" rel="stylesheet" />
    <link href="~/assets/media/slick/slick-theme.css" rel="stylesheet" />

    <link href="~/assets/plugins/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" />
    <link href="~/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" />
    <link href="~/assets/plugins/custom/datatables/fixedHeader.dataTables.min.css" rel="stylesheet" />
    <link href="~/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/plugins/global/plugins.bundle_icons.css" rel="stylesheet" />
    <link href="~/assets/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/bootstrap-tagsinput.css" rel="stylesheet" />
    <link href="~/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" />
    <link href="~/assets/plugins/custom/datatables/fixedHeader.dataTables.min.css" rel="stylesheet" />



    <link href="~/assets/css/Common.css" rel="stylesheet" />

</head>
<body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed toolbar-enabled toolbar-fixed aside-enabled aside-fixed" style="--kt-toolbar-height:55px;--kt-toolbar-height-tablet-and-mobile:55px">
    <!--data-kt-aside-minimize="on"-->
    <div class="d-flex flex-column flex-root" id="main-content">
        
        <div class="page d-flex flex-row flex-column-fluid">
            <!--begin::Aside-->
            <!--end::Aside-->
            <!--begin::Wrapper-->
            @* <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper" style="display:none!important; left:0px;padding-left:0px;"> *@
            <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper" style=" left:0px;padding-left:0px;">
                <!--begin::Header-->
                <div id="kt_header" style="left:0px;" class="header align-items-stretch">
                    <!--begin::Container-->
                    <div class="container-fluid d-flex align-items-stretch justify-content-between">
                        <!--begin::Aside mobile toggle-->
                        <div class="d-flex align-items-stretch justify-content-between flex-lg-grow-1 ">
@*                             <div class="btn btn-icon btn-active-light-primary w-30px h-30px w-md-40px h-md-40px" id="kt_aside_mobile_toggle">
                                <!--begin::Svg Icon | path: icons/duotune/abstract/abs015.svg-->
                                <span class="svg-icon svg-icon-1">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                        <path d="M21 7H3C2.4 7 2 6.6 2 6V4C2 3.4 2.4 3 3 3H21C21.6 3 22 3.4 22 4V6C22 6.6 21.6 7 21 7Z" fill="currentColor" />
                                        <path opacity="0.3" d="M21 14H3C2.4 14 2 13.6 2 13V11C2 10.4 2.4 10 3 10H21C21.6 10 22 10.4 22 11V13C22 13.6 21.6 14 21 14ZM22 20V18C22 17.4 21.6 17 21 17H3C2.4 17 2 17.4 2 18V20C2 20.6 2.4 21 3 21H21C21.6 21 22 20.6 22 20Z" fill="currentColor" />
                                    </svg>
                                </span>
                                <!--end::Svg Icon-->
                            </div> *@
                            <div style="align-content: center;">
                                <a href="#" class="" style="align-content: center;">
                                    <img id="companylogo" src="" class="logo" alt="Lighthouse e Solutions pte.ltd." style="max-width:220px;" fetchpriority="high">
                                    <noscript><img src="/assets/media/logos/con-lash-logo-01.svg" class="attachment-full size-full" alt="Company Logo" style="max-width:220px;" fetchpriority="high" /></noscript>
                                </a>
                            </div>
                            </div>
                        <!--end::Aside mobile toggle-->
                        <!--begin::Wrapper-->
                        <div class="d-flex align-items-stretch justify-content-between flex-lg-grow-1">
                            <!--begin::Navbar-->
                            


                            
                            <div class="d-flex align-items-center ms-auto me-10" style="margin-top:10px;">
                               @*  <div class="col-md-4">
                                    <label class="form-label fw-bolder text-gray-700 fs-7" style="margin: inherit;align-content: center;">Company: Con-Lash Supplies Pte Ltd</label>

                                </div> *@
                                <div class="gy-5 g-xl-9" style="width:auto;">
                                    <div class="mb-1 row form-group">
                                        <div class="col-md-12">
                                            <label  class="form-label fw-bolder text-gray-700 fs-7">Company: @UserData.EpodUser.les_company_details.Company_Description </label>
@*                                             <label id="slcompany" class="form-label fw-bolder text-gray-700 fs-7" style="margin: inherit;align-content: center;"></label>
 *@
                                            @*<select id="ddlSelCompany" class="form-select form-select-sm fs-7" data-control="select2" data-allow-clear="false" data-hide-search="false" data-placeholder="Select Company...">
                                                <option></option>
                                                
                                            </select>*@
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <!--end::Navbar-->
                            <!--begin::Toolbar wrapper-->
                            <div class="d-flex align-items-stretch flex-shrink-0">
                                <!--begin::User Name-->
                                <div class="d-flex align-items-center ms-1 ms-lg-3">
                                    <div class="fw-bolder d-flex align-items-center fs-6 text-gray-700" id="iUserName">
                                        @UserData.EpodUser.username
                                    </div>
                                </div>
                                <!--end::User Name-->
                                <!--begin::User menu-->
                                <div class="d-flex align-items-center ms-1 ms-lg-3" id="kt_header_user_menu_toggle">
                                    <!--begin::Menu wrapper-->
                                    <div class="cursor-pointer symbol symbol-30px symbol-md-30px" data-kt-menu-trigger="click" data-kt-menu-attach="parent" data-kt-menu-placement="bottom-end">
                                        <img src="~/assets/media/avatars/UserUnknown.png" alt="user" />
                                    </div>
                                    <!--begin::User account menu-->
                                    <div class="menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-800 menu-state-bg menu-state-primary fw-bold py-4 fs-6 w-350px" data-kt-menu="true">
                                        <!--begin::Menu item-->
                                        <div class="menu-item px-3">
                                            <div class="menu-content d-flex align-items-center px-3">
                                                <!--begin::Avatar-->
                                                <div class="symbol symbol-50px me-5">
                                                    <img alt="Logo" src="~/assets/media/avatars/userUnknown.png" />
                                                </div>
                                                <!--end::Avatar-->
                                                <!--begin::Username-->
                                                <div class="d-flex flex-column">
                                                    <div class="fw-bolder d-flex align-items-center fs-5" id="iloginuser">
                                                        @UserData.EpodUser.username
                                                        <span class="badge badge-light-success fw-bolder fs-8 px-2 py-1 ms-2">Active</span>
                                                    </div>
                                                    <a href="#" class="fw-bold text-muted text-hover-primary fs-7" id="iUserEmail">
                                                        @UserData.EpodUser.useremailid
                                                    </a>
                                                    <a href="#" class="fw-bold text-muted text-hover-primary fs-7" id="iUserCompany">
                                                        @UserData.EpodUser.les_company_details.Company_Description
                                                    </a>
                                                </div>
                                                <!--end::Username-->
                                            </div>
                                        </div>
                                        <!--end::Menu item-->
                                       
                                        <!--begin::Menu separator-->
                                        <div class="separator my-2"></div>
                                        <!--end::Menu separator-->
                                        <!--begin::Menu item-->
                                        <div class="menu-item px-5">
                                            <a class="menu-link px-2" id="btnepodlogout">Sign Out</a>
                                        </div>
                                        <!--end::Menu item-->
                                    </div>
                                    <!--end::User account menu-->
                                    <!--end::Menu wrapper-->
                                </div>
                                <!--end::User menu-->
                                <!--begin::Header menu toggle-->
                                <div class="d-flex align-items-center d-lg-none ms-2 me-n3 hide" title="Show header menu">
                                    <div class="btn btn-icon btn-active-light-primary w-30px h-30px w-md-40px h-md-40px" id="kt_header_menu_mobile_toggle">
                                        <!--begin::Svg Icon | path: icons/duotune/text/txt001.svg-->
                                        <span class="svg-icon svg-icon-1">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                <path d="M13 11H3C2.4 11 2 10.6 2 10V9C2 8.4 2.4 8 3 8H13C13.6 8 14 8.4 14 9V10C14 10.6 13.6 11 13 11ZM22 5V4C22 3.4 21.6 3 21 3H3C2.4 3 2 3.4 2 4V5C2 5.6 2.4 6 3 6H21C21.6 6 22 5.6 22 5Z" fill="currentColor" />
                                                <path opacity="0.3" d="M21 16H3C2.4 16 2 15.6 2 15V14C2 13.4 2.4 13 3 13H21C21.6 13 22 13.4 22 14V15C22 15.6 21.6 16 21 16ZM14 20V19C14 18.4 13.6 18 13 18H3C2.4 18 2 18.4 2 19V20C2 20.6 2.4 21 3 21H13C13.6 21 14 20.6 14 20Z" fill="currentColor" />
                                            </svg>
                                        </span>
                                        <!--end::Svg Icon-->
                                    </div>
                                </div>
                                <!--end::Header menu toggle-->
                            </div>
                            <!--end::Toolbar wrapper-->
                        </div>
                        <!--end::Wrapper-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Header-->
                <!--begin::Content-->
                <div class="content d-flex flex-column flex-column-fluid pt-5 pb-0" id="kt_content">
                    <!--begin::Toolbar-->
                    <div class="toolbar" id="kt_toolbar" style="left:0px;">
                        <!--begin::Container-->
                        <div id="kt_toolbar_container" class="container-fluid d-flex flex-stack">
                            <!--begin::Page title-->
                            <div data-kt-swapper="true" data-kt-swapper-mode="prepend" data-kt-swapper-parent="{default: '#kt_content_container', 'lg': '#kt_toolbar_container'}" class="page-title d-flex align-items-center flex-wrap me-3 mb-5 mb-lg-0">
                                <!--begin::Title-->
                                <!--begin::Breadcrumb-->
                                <ul class="breadcrumb breadcrumb-separatorless fw-bold fs-7 my-1">
                                    <!--begin::Item-->
                                    <li class="breadcrumb-item text-primary fw-bolder">
                                        <a href="~/Home/Index" class="text-primary">Home</a>
                                    </li>
                                    <!--end::Item-->
                                    <!--begin::Item-->
                                    @if (ViewBag.SubTitle != null && ViewBag.SubTitle != "")
                                    {
                                        <li class="breadcrumb-item text-dark fw-bolder" id="breadcrumbsubtitle">
                                            <span class="bullet bg-gray-300 w-5px h-2px" style="margin-right: 5px;"></span>
                                            <a href="@ViewBag.SubTitleUrl" class="text-primary"> @ViewBag.SubTitle</a>
                                            @* <a class="text-primary"> @ViewBag.SubTitle</a>*@
                                        </li>
                                    }
                                    <li class="breadcrumb-item">
                                        <span class="bullet bg-gray-300 w-5px h-2px"></span>
                                    </li>
                                    <!--end::Item-->
                                    <li class="breadcrumb-item text-dark fw-bolder" id="breadcrumbtitle">
                                      
                                        @ViewBag.Title
                                    </li>
                                    <!--begin::Item-->
                                    <!--end::Item-->
                                </ul>
                                <!--end::Breadcrumb-->
                            </div>

                            <!--end::Page title-->
                            <!--begin::Actions-->
                            <div class="d-flex align-items-center gap-2 gap-lg-3">
                                @RenderSection("ActionList", false)
                            </div>
                            <!--end::Actions-->
                        </div>
                        <!--end::Container-->
                    </div>
                    @if (ViewBag.ReferenceNo != null && ViewBag.ReferenceNo != "")
                    {
                        <div class="toolbar" id="kt_toolbar1" style="top: 120px;height: 30px;">
                            <!--begin::Container-->
                            <div id="kt_toolbar_container" class="container-fluid d-flex flex-stack">
                                <div class="breadcrumb-item text-danger fw-bolder" style="padding:5px; font-size: .95rem!important;" id="RefNo">
                                    <span class="h-20px text-danger border-start mx-2"></span>
                                    Reference No: @ViewBag.ReferenceNo
                                </div>
                            </div>
                        </div>
                    }
                    <!--end::Toolbar-->
                    <!--begin::Post-->
                    <div class="post d-flex flex-column-fluid" id="kt_post">
                        <!--begin::Container-->
                        <div id="kt_content_container" class="container-xxl mx-0 px-5" style="max-width:100% !important;">
                            <!--begin::Row-->
                            <div class="row gy-4 gx-4 g-xl-4">
                                <!--begin::Col-->
                                <div class="col-xl-12">
                                    <!--begin::Tables Widget -->
                                        <div id="baseCard" class="card">
                                            <!--begin::Body-->
                                            <div class="card-body py-3 px-5">
                                               
                                                @RenderBody()
                                            </div>
                                            <!--begin::Body-->
                                        </div>
                                    <!--end::Tables Widget -->
                                </div>
                                <!--end::Col-->
                            </div>
                            <!--end::Row-->
                        </div>
                        <!--end::Container-->
                    </div>
                    <!--end::Post-->
                </div>
                <!--end::Content-->
                <!--begin::Footer-->
                <div class="footer py-4 d-flex flex-lg-column" id="kt_footer">
                    <!--begin::Container-->
                    <div class="d-flex flex-column" style="margin-left:10px;">

                        <div class="text-center">
                            <div class="d-flex flex-column align-items-start mt-3 copyright">
                                @{

                                    if (UserData.logoflag == 1)
                                    {
                                        <!--begin::Logo-->
                                        <a class="fs-lg-7 fs-md-7 fs-8 text-gray-700" href="https://lesconnect.io/" target="_blank">
                                            Powered By <img alt="Logo" src="~/assets/media/logos/LeSConnectLogos_MainLogo.png" class="h-25px h-md-30px">
                                        </a>
                                        <!--end::Logo image-->
                                    }
                                }
                                
                                <!--begin::Version-->
                                <span class="fs-lg-7 fs-md-7 fs-8 fw-bold text-gray-700">Version - @UserData.projectversion</span>
                                <!--end::Version-->
                            </div>
                        </div>
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Footer-->
            </div>
        </div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    @* <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script> *@

    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/plugins/global/plugins.bundle.js"></script>
    <script src="~/assets/js/scripts.bundle.js"></script>
    <script src="~/assets/plugins/custom/prismjs/prismjs.bundle.js"></script>
    <script src="~/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js"></script>
    <script src="~/assets/plugins/custom/datatables/datatables.bundle.js"></script>
    <script src="~/assets/plugins/custom/tagsinput/bootstrap-tagsinput.js"></script>
    <script src="~/assets/media/slick/slick.min.js"></script>
    <script>
        var UserDefaultData = @Html.Raw(Json.Serialize(UserData));
    </script>
    <script src="~/assets/js/custom/epodcommon.js"></script>
    @* <script src="~/assets/js/Common.js"></script> *@
    @RenderSection("scripts", required: false)
    @RenderSection("Modals", required: false)
</body>
</html>